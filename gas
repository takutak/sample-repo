function honnbann() {
  //ここでシートのまとまりを取得。稼働中のものに対して実行されるので注意
  var spreadsheet = SpreadsheetApp.getActive();
  
  //ページを取得しいく.以後も使うのでここが重要
  const sheet = spreadsheet.getSheetByName("夏講習");
  const sheet1 = spreadsheet.getSheetByName("報告書10");
  const sheet2 = spreadsheet.getSheetByName("報告書20");

  //ここまでの確認
  //Logger.log(sheet2.getName()); →大丈夫だった。

  //アクセス回数が多くなるので、データの取得は配列で実施
  const data = sheet.getRange('E2');
  console.log(data.getValue());
  //ここは情報取得できるかお試し中


  spreadsheet.setActiveSheet(spreadsheet.getSheetByName('報告書10'), true);
  spreadsheet.getRange('B4').activate();
  spreadsheet.getRange('\'夏講習\'!B2:K2').copyTo(spreadsheet.getActiveRange(), SpreadsheetApp.CopyPasteType.PASTE_NORMAL, false);
  spreadsheet.getRange('B18:G18').activate();
  spreadsheet.getActiveRangeList().setBackground('#f6b26b');
};

